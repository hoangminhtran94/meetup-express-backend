// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Meetup {
  id           String  @id @default(auto()) @map("_id") @db.ObjectId
  title        String
  subtitle     String
  description  String
  imageUrl     String
  address      String
  contactEmail String
  isFavorite   Boolean
}
